{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "Fairly light, multi-version library used by all of LilBroCodes's mods.",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/composer_reloaded/icon.png",
  "environment": "*",
  "access_widener": "composer_reloaded.accesswidener",
  "entrypoints": {
    "fabric-datagen": [
      "org.lilbrocodes.composer_reloaded.internal.data.ComposerReloadedDataGenerator"
    ],
    "client": [
      "org.lilbrocodes.composer_reloaded.internal.client.ComposerReloadedClient"
    ],
    "main": [
      "org.lilbrocodes.composer_reloaded.internal.ComposerReloaded"
    ],
    "cardinal-components": [
      "org.lilbrocodes.composer_reloaded.internal.cca.ModCardinalComponents"
    ]
  },
  "mixins": [
    "composer_reloaded.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabricloader": ">=${loader}",
    "fabric-api": ">=${fapi}",
    "minecraft": "${minecraft}",
    "cardinal-components": ">=${cca}"
  },
  "custom": {
    "cardinal-components": [
      "composer_reloaded:targeted_block",
      "composer_reloaded:targeted_entity"
    ],
    "modmenu": {
      "badges": [ "library" ],
      "update_checker": false
    }
  }
}